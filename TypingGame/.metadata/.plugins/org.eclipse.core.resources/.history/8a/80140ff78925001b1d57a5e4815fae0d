package typing_game1;

import javax.swing.*;
import java.awt.*;

public class TypingGame extends JFrame{
	public TypingGame() {
		setTitle("Typing Game");
		setSize(Main.SCREEN_WIDTH, Main.SCREEN_HEIGHT);
		setResizable(false);
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		setVisible(true);
	}
}
